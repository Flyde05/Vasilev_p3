<Window x:Class="Vasilev_p3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vasilev_p3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Foreground="#243238" Height="42" Margin="10,10,286,52" Text="Введите a" Padding="12,12,12,12"/>
        <TextBlock Foreground="#243238" Height="42" Margin="10,10,264,51" Text="Введите b" Grid.Row="1" Padding="12,12,12,21"/>
        <Button Foreground="#fff6a5" Height="42" Grid.Row="2" Margin="10,27,286,32" Padding="12,12,12,12"  Click="Button_Click" >Решить</Button>

        <TextBox Height="42" x:Name="tbA" Padding="12,12,12,12" Margin="240,10,39,53" Text="2"/>
        <TextBox Height="42" x:Name="tbB" Padding="12,12,12,12" Margin="240,10,39,53" Grid.Row="1" Text="3"/>


    </Grid>
</Window>
